<!DOCTYPE html>
<html class=" js csstransforms3d"><head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <title>充电桩管理</title>
    <link rel="stylesheet" href="css/page.css">
    <!--[if lte IE 8]>
    <![endif]-->
    <script type="text/javascript" src="js/jquery.min.js"></script>
    <script type="text/javascript" src="http://api.map.baidu.com/api?v=2.0&ak=MkGAxwmEtETTE1U8ievYOLlAVFg7Vz1F"></script>
</head>
<body style="background: rgb(246, 245, 250);">
<!--content S-->
<div class="super-content">

    <div class="superCtab">
        <div class="publishArt">
            <h4>充电桩管理</h4>
            <div class="pubMain">
                <form action="" method="post">
                    <h5 class="pubtitle">选择分类</h5>
                    <div class="pubselect">
                        <b class="select_arrow"></b>
                        开放等级
                        <select  class="navOne select_ui" style="height: 38px;outline: none">
                            <option>dddddddddd</option>
                            <option>dddddddddd</option>
                            <option>dddddddddd</option>
                            <option>dddddddddd</option>
                        </select>
                        <b class="select_arrow"></b>
                        使用类型
                        <select  class="navTwo select_ui" style="height: 38px;outline: none">
                            <option>dddddddddd</option>
                            <option>dddddddddd</option>
                            <option>dddddddddd</option>
                            <option>dddddddddd</option>
                        </select>
                    </div>

                    <h5 class="pubtitle">地图</h5>
                    <div class="pub-area-bar">
                        <div id="salesmap"></div>
                    </div>
                    <div class="pub-area-bar" style="width: 60%;margin-top: 30px">
                        <span style="display: inline-block;width: 24%">经度:<input type="text"  style="width: 80%;border: 1px solid #d8d8d8;padding: 10px 10px;height: 17px;outline: none;" name="longitude" readonly></span>
                        <span style="display: inline-block;width: 24%">纬度:<input type="text"  style="width: 80%;border: 1px solid #d8d8d8;padding: 10px 10px;height: 17px;outline: none;" name="latitude" readonly></span>
                        <span style="display: inline-block;width: 24%">省份:<input type="text"  style="width: 80%;border: 1px solid #d8d8d8;padding: 10px 10px;height: 17px;outline: none;" name="province" readonly></span>
                        <span style="display: inline-block;width: 24%">城市:<input type="text"  style="width: 80%;border: 1px solid #d8d8d8;padding: 10px 10px;height: 17px;outline: none;" name="city" readonly></span>
                    </div>

                    <div class="pub-btn">
                        <input type="button" id="add" value="添加" class="saveBtn"/>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="js/chargingstation.js"></script>
</body></html>