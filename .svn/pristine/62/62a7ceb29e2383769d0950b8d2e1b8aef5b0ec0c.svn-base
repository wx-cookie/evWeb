$(function(){
	(function(){
		var $li = $('.ctab-Main-title ul li');
		$.each($li,function(i,v){
			if($(this).children(".secondList")){
				$(this).mouseover(function(){
				$(this).children(".secondList").css("display","block");
				});
				$(this).mouseout(function(){
				$(this).children(".secondList").css("display","none");
			})
			}
		})
	})();
});