/*条件筛选页面样式*/
.car-side{
	margin-top:50px;
	float: left;
    width: 240px;
    height: 30px;
    margin-right: 10px;
}
.car-side .side .fix{
	position: fixed;
    top: 0px;
}

.letter-list-box{
	width: 238px;
    border: 1px solid #e9e9e9;
    border-radius: 5px;
    background: #fff;
}
.letter-list-box ul{
	margin: 14px 0px 9px 14px;
    height: auto;
    overflow: auto;
}
.letter-list-box ul li{
    display: inline-block;
    vertical-align: top;
    width: 24px;
    line-height: 24px;
    height: 24px;
    border: 1px solid #e9e9e9;
    border-radius: 3px;
    text-align: center;
    float: left;
    margin: 0px 5px 5px 0px;
}
.letter-list-box ul li a{
	display:block;
	text-align:center;
	color:#666;
	font-size:14px;
	border-radius:3px;
}
.letter-list-box ul .letter-list.active a,
.letter-list-box ul li a:hover{
	background:#404040;
	color: #fff;
}
.brand-nav-box dl dd a:hover, .brand-nav-box dl dd li.active a{
	background: #404040;
    color: #fff;
}
.brand-nav-box{
    background: #fff;
    border: 1px solid #e9e9e9;
    height: 400px;
    overflow: auto;
}
.brand-nav-box::-webkit-scrollbar{
	width: 10px;
    margin-right: -5px;
    background-color: rgba(0,0,0,0.2);
}
.brand-nav-box::-webkit-scrollbar-thumb{
	border-radius: 5px;
    background-color: #404040;
}
.brand-nav-box::-webkit-scrollbar-track{
	border-radius: 55px;
    background-color: rgba(0,0,0,0);
}
.brand-nav-box dl dt{
	padding: 0px 15px;
    border-bottom: 1px solid #e9e9e9;
    border-top: 1px solid #e9e9e9;
    font-size: 22px;
    font-weight: bold;
}
.brand-nav-box dl dd a{
	color:#666666;
	font-family:MicrosoftYaHei;
	display:block;
	padding: 0px 15px;
    line-height: 35px;
    font-size: 14px;
}

.car-container{
	width:960px;
	float:left;
	margin-top:50px;
}
#condition-box{
	border: 1px solid #e9e9e9;
    border-radius: 5px;
    width: 948px;
    background: #fff;
    top: -100px;
}
.OdCarConditionContainer{
	overflow: hidden;
    margin: 0px 10px;
}
.OdCarConditionContainer .OdCarCondition{
	display: table;
    line-height: 0;
    padding: 10px 0px 5px
}
.OdCarConditionContainer .OdCarCondition  dd .custom-price{
	display: inline-block;
    border-radius: 3px;
    color: #666;
    font-size: 12px;
    transition: all 0.2s linear;
    margin: 0px 5px 5px 10px;
}
.OdCarConditionContainer .OdCarCondition  dd .custom-price input{
	display: inline-block;
    border: none;
    border: 1px solid #d8d8d8;
    width: 48px;
    padding: 0px;
    border-radius: 3px;
    background: none;
    text-align: center;
    line-height: 25px;
    height: 25px;
    vertical-align: middle;
    font-size: 12px;
	outline: none;
    resize: none;
    font: 14px/1.5 "Helvetica Neue",Helvetica,Arial,"Microsoft Yahei","Hiragino Sans GB","Heiti SC","WenQuanYi Micro Hei",sans-serif;
}
.OdCarConditionContainer .OdCarCondition  dd .custom-price a{
	display: inline-block;
    vertical-align: middle;
    line-height: 27px;
    width: 50px;
    margin-left: 5px;
    color: #fff;
    background: #404040;
    text-align: center;
    border-radius: 3px;
	transition: all 0.2s linear;
}
.OdCarConditionContainer .OdCarCondition  dt{
	width: 70px;
    text-align: right;
    font-size: 14px;
    line-height: 30px;
    color: #999;
}
.OdCarConditionContainer .OdCarCondition  dd{
	text-align: left;
}
.OdCarConditionContainer .OdCarCondition  dt, .OdCarConditionContainer .OdCarCondition  dd{
	display: table-cell;
    vertical-align: top;
}
.OdCarConditionContainer .OdCarCondition  dd .btn{
	display: inline-block;
    border-radius: 3px;
    /* border: 1px solid #e9e9e9; */
    line-height: 28px;
    height: 28px;
    overflow: hidden;
    /* width: 83px; */
    text-align: center;
    color: #666;
    font-size: 14px;
    vertical-align: top;
    transition: all 0.1s linear;
    padding: 0px 10px;
    margin-bottom: 3px;
    margin-left: 3px;
}
.OdCarConditionContainer .OdCarCondition  dd .sp{
	display: inline-block;
    border-radius: 3px;
    /* border: 1px solid #e9e9e9; */
    line-height: 28px;
    height: 28px;
    overflow: hidden;
    /* width: 83px; */
    text-align: center;
    color: #666;
    font-size: 14px;
    vertical-align: top;
    transition: all 0.1s linear;
    padding: 0px 10px;
    margin-bottom: 3px;
    margin-left: 3px;
}
.OdCarConditionContainer .OdCarCondition  dd .multi .multi-arrow{
	display: inline-block;
    font-family: 'lsjcar';
    vertical-align: top;
    line-height: 12px;
    margin-left: 5px;
    margin-top: 7px;
}
/*.OdCarConditionContainer .OdCarCondition  dd .multi .multi-arrow:after{
	content: '\5D';
	font-size: 12px;
}*/
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate-box{
	position: absolute;
    /*z-index: -5;*/
    margin-left: -10px;
   /* left: -100000px;*/
}
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate{
	background: #fff;
    font-size: 12px;
    border-radius: 3px;
    border: 1px solid #e9e9e9;
    padding: 10px 0px;
    box-shadow: 0px 3px 10px 0px rgba(0,0,0,0.26);
    /*opacity: 0;*/
    margin-top: 0px;
    transition: opacity 0.2s linear,margin-top 0.2s linear;
}
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate li{
	text-align: left;
    margin-bottom: 0px;
	list-style: none;
}
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate .small-cate-btn{
	display: inline-block;
	line-height: 25px;
	color: #fff;
	background: #404040;
	font-size: 12px;
	text-align: center;
	width: 46px;
	border-radius: 3px;
	cursor: pointer;
}
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate .small-cate-item{
	padding:0px 20px;
	text-align: center;
	display: block;
	transition: all 0.2s linear;
}
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate .small-cate-item:hover,
.OdCarConditionContainer .OdCarCondition  dd .multi .small-cate .small-cate-item.active{
	background: #404040;
	color: #fff;
}
.OdCarConditionContainer .OdCarCondition  dd .btn.active{
	border-color: #404040;
    background: #404040;
